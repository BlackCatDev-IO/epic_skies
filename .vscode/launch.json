  "version": "0.2.0",
  "configurations": [
    {
      "name": "epic_skies",
      "type": "dart",
      "request": "launch"
    },
    {
      "name": "epic_skies_profile",
      "type": "dart",
      "request": "launch",
      "flutterMode": "profile"
    },

    {
      "name": "epic_skies_release",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "Launch Staging",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main.dart",
      "args": ["--flavor", "staging", "--target", "lib/main.dart"]
    },
    {
      "name": "Launch Staging-Debug",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--flavor", "staging", "--target", "lib/main.dart"]
    }
  ]
}
